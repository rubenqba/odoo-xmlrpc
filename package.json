        "email": "111847+rubenqba@users.noreply.github.com"
    },
    "homepage": "https://github.com/rubenqba/odoo-xmlrpc#readme",
    "devDependencies": {
        "@types/chai": "^4.3.5",
        "@types/express": "^4.17.17",
        "@types/mocha": "^10.0.1",
        "@types/xmlrpc": "^1.3.7",
        "@typescript-eslint/eslint-plugin": "^5.60.1",
        "@typescript-eslint/parser": "^5.60.1",
