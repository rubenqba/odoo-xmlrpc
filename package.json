        "install-peers-cli": "^2.2.0",
        "mocha": "^10.2.0",
        "nyc": "^15.1.0",
        "prettier": "^2.8.8",
        "ts-node": "^10.9.1",
        "typescript": "^5.1.6"
    },
    "dependencies": {
        "xmlrpc": "^1.3.2"
    }
